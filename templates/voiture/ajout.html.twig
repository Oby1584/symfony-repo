{% extends "base.html.twig" %}

{% block title %}ajout voiture{% endblock %}

{% block body %}
    <h1 class="text-center my-4" >Ajout Voiture</h1>
<div class="container">
    {{ form_start(formVoiture)}}
        {{form_row(formVoiture.marque)}}
